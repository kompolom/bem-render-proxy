{
  "author": "Evgeniy Baranov <master@kompolom.ru>",
  "name": "bem-render-proxy",
  "version": "0.14.0",
  "description": "Render proxy for bem projects. Render data with BEMHTML and BEMTREE",
  "main": "dist/index.js",
  "repository": "https://github.com/kompolom/bem-render-proxy",
  "homepage": "https://github.com/kompolom/bem-render-proxy",
  "engines": {
    "node": ">=6.9"
  },
  "scripts": {
    "start": "node dist/index.js",
    "test": "jest",
    "build": "tsc"
  },
  "keywords": [
    "bem"
  ],
  "dependencies": {
    "cookie-parser": "^1.4.3",
    "express": "4.17.1",
    "morgan": "^1.6.1",
    "node-telegram-bot-api": "0.50.0",
    "request": "^2.81.0",
    "set-cookie-parser": "^2.0.0",
    "yn": "4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.1",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.5"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "dist"
    ],
    "testRegex": "(/test/.*\\.test)\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "@typescript-eslint/ban-ts-comment": "off"
    },
    "ignorePatterns": [
      "dist"
    ]
  },
  "license": "ISC"
}
